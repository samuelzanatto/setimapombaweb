datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  name      String
  email     String
  online    Boolean  @default(false)
  createdAt DateTime @default(now())
}
